<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE irprotocol PUBLIC "-//bengt-martensson.de//irprotocols//en"
 "../dtds/irprotocols.dtd">
<irprotocol name="panasonic" id="panasonic">
  <jp1data>
    <protocol number="00C9" name="panasonic"/> <!-- 001F 00C9 00CD -->
    <tohex function="reverse_complement" bits="8"/>
  </jp1data>
  <parameters>
    <parameter id="deviceno" bits="8"/>
    <parameter id="subdevice" bits="8"/>
    <parameter id="cmdno" bits="8"/>
  </parameters>
  <frequency value="37000"/>
  <irpnotation-new>
<![CDATA[
IRP notation: {37k,432}<1,-1,1,-3>(8,-4,2:8,32:8,D:8,S:8,F:8,(D^S^F):8,1,-173)+ 
]]>
  </irpnotation-new>
  <body>
    <timings>
      <timing id="t1" cycles="16"/>
      <timing id="t3" cycles="48"/>
      <timing id="t8" cycles="128"/>
      <timing id="t4" cycles="64"/>
      <timing id="t173" cycles="1384"/>
    </timings>
    <pairs>
      <pair id="p0">
        <one timing="t1"/>
        <zero timing="t1"/>
      </pair>
      <pair id="p1">
        <one timing="t1"/>
        <zero timing="t3"/>
      </pair>
    </pairs>
    <intro/>
    <repeat>
      <one timing="t8"/>
      <zero timing="t4"/>
      <coded_number direction="lsb" zero="p0" one="p1" bits="8">
	<constant value="2"/><!-- OEM code1 for Kaseiko -->
      </coded_number>
      <coded_number direction="lsb" zero="p0" one="p1" bits="8">
	<constant value="32"/><!-- OEM code2 for Kaseiko -->
      </coded_number>
      <coded_number direction="lsb" zero="p0" one="p1" bits="8">
	<parameterref parameter="deviceno"/>
      </coded_number>
      <coded_number direction="lsb" zero="p0" one="p1" bits="8">
	<parameterref parameter="subdevice"/>
      </coded_number>
      <coded_number direction="lsb" zero="p0" one="p1" bits="8">
	<parameterref parameter="cmdno"/>
      </coded_number>
      <coded_number direction="lsb" zero="p0" one="p1" bits="8">
	<xor>
	  <parameterref parameter="deviceno"/>
	  <parameterref parameter="subdevice"/>
	  <parameterref parameter="cmdno"/>
	</xor>
      </coded_number>
      <one timing="t1"/>
      <zero timing="t173"/>
     </repeat>
  </body>
</irprotocol>
