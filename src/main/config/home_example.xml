	  </zone>
	  <zone name="3">
	    <selectcommand audio_video="zone3_vcr"/>
	    <querycommand cmd="get_zone3" val="Z3VCR"/>
	  </zone>
	  <zone name="record">
	    <selectcommand audio_video="recordsrc_vcr"/>
