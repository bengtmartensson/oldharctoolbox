	    <selectcommand audio_video="in_dvr" video_only="videoinput_dvr"/>
        <querycommand cmd="get_input" val="SIDVR"/>
	    <querycommand cmd="get_videoinput" mediatype="video_only" val="SVDVR"/>
	  </zone>
	  <zone name="2">
	    <selectcommand audio_video="zone2_dvr"/>
	    <querycommand cmd="get_zone2" val="Z2VCR"/>
	  </zone>
	  <zone name="3">
	    <selectcommand audio_video="zone3_dvr"/>
	    <querycommand cmd="get_zone3" val="Z3VCR"/>
	  </zone>
	  <zone name="record">
	    <selectcommand audio_video="recordsrc_dvr"/>
	    <querycommand cmd="get_recordsrc" val="SRDVR"/>
	  </zone>
	</input>
	<input name="phono" video="no">
	  <zone name="1">
	    <selectcommand audio_video="in_phono" audio_only="in_phono"/>
	    <querycommand cmd="get_input" val="SIPHONO"/>
	  </zone>
	  <zone name="2">
	    <selectcommand audio_video="zone2_phono" audio_only="zone2_phono"/>
	    <querycommand cmd="get_zone2" val="Z2PHONO"/>
	  </zone>
	  <zone name="3">
	    <selectcommand audio_video="zone3_phono" audio_only="zone3_phono"/>
	    <querycommand cmd="get_zone3" val="Z3PHONO"/>
	  </zone>
	  <zone name="record">
	    <selectcommand audio_video="recordsrc_phono"/>
	    <querycommand cmd="get_recordsrc" val="SRPHONO"/>
	  </zone>
	</input>
	<input name="cd" myname="tv/pc" video="no">
	  <zone name="1">
	    <selectcommand audio_video="in_cd" audio_only="in_cd"/>
	    <querycommand cmd="get_input" val="SICD"/>
	  </zone>
	  <zone name="2">
	    <selectcommand audio_video="zone2_cd" audio_only="zone2_cd"/>
