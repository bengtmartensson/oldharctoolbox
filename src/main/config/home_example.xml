<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE homemap PUBLIC "-//bengt-martensson.de//home//en" "../dtds/home.dtd">
<!--
Description of devices, applications, gateways, groups, zones.

This is a sample file.
-->
<homemap>
  <aliases>
    <alias id="main-receiver" device="denon"/>
    <alias id="receiver" device="denon"/>
    <alias id="amp" device="denon"/>
    <alias id="sat-receiver" device="dbox"/>
    <alias id="cable-receiver" device="dbox"/>
    <alias id="digital-receiver" device="dbox"/>
    <alias id="dvd" device="oppo"/>
    <alias id="bluray" device="samsung"/>
    <alias id="tv" device="philips_37pfl9603"/>
    <alias id="projector" device="z2000"/>
  </aliases>
  <device-groups>
    <device-group id="light" name="Light">
      <deviceref device="front_light"/>
      <deviceref device="work_light"/>
      <deviceref device="hall_light"/>
      <deviceref device="kitchen_light"/>
    </device-group>

    <device-group id="blinds" name="Blinds">
      <deviceref device="large_blind"/>
      <deviceref device="bedroom_blind"/>
    </device-group>

    <device-group id="misc" name="Misc.">
      <deviceref device="balcony_door"/>
      <deviceref device="screen"/>
      <deviceref device="x10-N1"/>  
    </device-group>

    <device-group id="living_room" name="Living Room">
      <deviceref device="front_light">Front Lightning</deviceref>
      <deviceref device="work_light">Work Lightning</deviceref>
      <deviceref device="balcony_door">Balcony Door</deviceref>
      <deviceref device="screen"/>
      <deviceref device="large_blind"/>
    </device-group>

    <device-group id="kitchen_hall_bedroom" name="Kitchen/Hall/Bedroom">
      <deviceref device="kitchen_light"/>
      <deviceref device="hall_light"/>
      <deviceref device="bedroom_blind"/>
    </device-group>

    <device-group id="bedroom" name="Bedroom zone" zone="2">
      <!--from-gateway gateway="globalcache" connector="2"/-->
      <deviceref device="yamaha_bedroom"/>
      <deviceref device="pioneer_cld925"/>
      <deviceref device="marantz_cd42"/>
      <deviceref device="marantz_tape"/>
      <deviceref device="panasonic_dvd_s75"/>
      <deviceref device="tiny_tv"/>
    </device-group>

    <device-group id="deskzone" name="desk" zone="3">
       <!--from-gateway gateway="globalcache" connector="3"/-->
       <deviceref device="yamaha_desk"/>
    </device-group>

    <device-group id="main" name="Main Devices" zone="1">
      <deviceref device="denon"/>
      <!--deviceref device="pioneer_cld925"/-->
      <deviceref device="oppo"/>
      <deviceref device="popcorn"/>
      <!--deviceref device="panasonic_dvd_s75"/-->
      <deviceref device="samsung"/>
      <deviceref device="dbox"/>
      <!--deviceref device="philips_28ml8805"/-->
      <deviceref device="philips_37pfl9603"/>
      <deviceref device="z2000"/>
    </device-group>

    <device-group id="gateways" name="Intelligent Gateways">
      <deviceref device="gc_dev"/>
      <deviceref device="gc2_dev"/>
      <deviceref device="irtrans_dev"/>
      <deviceref device="t10_dev"/>
    </device-group>
  </device-groups>

  <devices>
    <device id="denon" class="denon_avr3808" name="Denon AVR-3808">
      <!-- Here, order is significant, and denotes preference.
      In particular, make sure that www comes last! -->
      <gateway-port gateway="lan" connectortype="tcp" hostname="denon" portnumber="23"/>
      <gateway-port-ref gateway-port="globalcache_mainzone"/>
      <gateway-port-ref gateway-port="irtrans_mainzone"/>
      <gateway-port gateway="lan" connectortype="ip" hostname="denon" wol="no"/>
      <gateway-port gateway="lan" connectortype="www" hostname="denon" portnumber="80"/>
      <inputs>
	<!-- Order in usability order -->
	<input name="sat" myname="dBox" audio="yes" video="yes">
	  <zone name="1">
	    <selectcommand audio_video="in_sat" video_only="videoinput_sat"/>
	    <querycommand cmd="get_input" val="SISAT"/>
	    <querycommand cmd="get_videoinput" mediatype="video_only" val="SVSAT"/>
	  </zone>
	  <zone name="2">
	    <selectcommand audio_video="zone2_sat"/>
	    <querycommand cmd="get_zone2" val="Z2SAT"/>
	  </zone>
	  <zone name="3">
	    <selectcommand audio_video="zone3_sat"/>
	    <querycommand cmd="get_zone3" val="Z3SAT"/>
	  </zone>
	  <zone name="record">
	    <selectcommand audio_video="recordsrc_sat"/>
	    <querycommand cmd="get_recordsrc" val="SRSAT"/>
	  </zone>
	  
          <connector connectiontype="cvbs">
	    <!--deviceref device="dbox"/-->
	    <deviceref device="dbox"/>
	  </connector>
	  <connector connectiontype="analog">
	    <!--deviceref device="dbox"/-->
	    <deviceref device="dbox"/>
	  </connector>
	  <connector connectiontype="spdif" hardware="optical" number="1">
	    <!--deviceref device="dbox"/-->
	    <deviceref device="dbox"/>
	  </connector>
	  <!--connector connectiontype="yuv" number="3">
	    <deviceref device="dbox"/>
	  </connector-->
	  <connector connectiontype="hdmi" number="3">
	    <deviceref device="dbox"/>
	  </connector>
	</input>
	<input name="dvd" audio="yes" video="yes">
	  <zone name="1">
	    <selectcommand audio_video="in_dvd" video_only="videoinput_dvd"/>
	    <querycommand cmd="get_input" val="SIDVD"/>
	    <querycommand cmd="get_videoinput" mediatype="video_only" val="SVDVD"/>
	  </zone>
	  <zone name="2">
	    <selectcommand audio_video="zone2_dvd"/>
	    <querycommand cmd="get_zone2" val="Z2DVD"/>
	  </zone>
	  <zone name="3">
	    <selectcommand audio_video="zone3_dvd"/>
	    <querycommand cmd="get_zone3" val="Z3DVD"/>
	  </zone>
	  <zone name="record">
	    <selectcommand audio_video="recordsrc_dvd"/>
	    <querycommand cmd="get_recordsrc" val="SRDVD"/>
	  </zone>
	   <connector connectiontype="cvbs">
	    <deviceref device="oppo"/>
	  </connector>
	  <connector connectiontype="analog">
	    <deviceref device="oppo"/>
	  </connector>
	  <connector connectiontype="external_analog">
	    <deviceref device="oppo"/>
	  </connector>
	  <!--connector connectiontype="spdif" hardware="optical" number="2">
	  </connector-->
	  <!--connector connectiontype="yuv" number="1">
	    <deviceref device="oppo"/>
	  </connector-->
	  <connector connectiontype="hdmi" number="2">
	    <deviceref device="oppo"/>
	  </connector>
	</input>
	<input name="hdp" myname="Blu Ray" audio="yes" video="yes">
	  <zone name="1">
	    <selectcommand audio_video="in_hdp" video_only="videoinput_hdp"/>
	    <querycommand cmd="get_input" val="SIHDP"/>
	    <querycommand cmd="get_videoinput" mediatype="video_only" val="SVHDP"/>
	  </zone>
	  <zone name="2">
	    <selectcommand audio_video="zone2_hdp"/>
	    <querycommand cmd="get_zone2" val="Z2HDP"/>
	  </zone>
	  <zone name="3">
	    <selectcommand audio_video="zone3_hdp"/>
	    <querycommand cmd="get_zone3" val="Z3HDP"/>
	  </zone>
	  <zone name="record">
	    <selectcommand audio_video="recordsrc_hdp"/>
	    <querycommand cmd="get_recordsrc" val="SRHDP"/>
	  </zone>
	  
	</input>
	<input name="dvr" myname="HD-DVD" audio="yes" video="yes">
