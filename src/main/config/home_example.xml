	  <!--deviceref device="philips_37pfl9603"/-->
	</input>
	<input name="tuner" video="no">
	  <zone name="1">
	    <selectcommand audio_video="in_tuner" audio_only="in_tuner"/>
	    <querycommand cmd="get_input" val="SITUNER"/>
	  </zone>
	  <zone name="2">
	    <selectcommand audio_video="zone2_tuner" audio_only="zone2_tuner"/>
