<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE irprotocol PUBLIC "-//bengt-martensson.de//irprotocols//en"
 "../dtds/irprotocols.dtd">
<irprotocol name="rc5x" id="rc5x">
  <!-- DecodeIR:
  What we call "Device" is really the "System". What we call Subdevice is really
  the "Command". What we call "OBC" is really the "Data". If you are using
  ProntoEdit to create the RC5 signals directly, that GUI uses that correct
  (System, Command and Data) Philips terminolgy.
  -->
  <jp1data>
    <protocol number="00 F2" name="RC5x"/>
    <tohex function="reverse-invert" bits="8"/>
  </jp1data>
  <cooked-ccf-string format="FIXME 5000 0073 0000 0001 %04x %04x">
    <parameterref parameter="deviceno"/>
    <parameterref parameter="cmdno"/>
  </cooked-ccf-string>
  <parameters>
    <parameter id="deviceno" name="system" bits="6"/>
    <parameter id="subdevice" name="command" bits="7"/>
    <parameter id="cmdno" name="data" bits="6"/>
    <parameter id="toggle" bits="1"/>
  </parameters>
  <frequency value="36000"/>
  <irpnotation-new>
RC5:  {36k,msb,889}&lt;1,-1|-1,1>(1:1,~F:1:6,T:1,D:5,F:6,^128)+
RC5x: {36k,msb,889}&lt;1,-1|-1,1>(1:1,~S:1:6,T:1,D:6,-4,S:6,F:6,^114m)+
(should be D:5 instead of D:6)
  </irpnotation-new>
  <body>
    <timings>
      <timing id="t1" cycles="32"/>
      <timing id="t4" cycles="128"/>
      <timing id="ttotal" cycles="4104"/>
    </timings>
    <pairs>
      <pair id="p0">
        <one timing="t1"/>
        <zero timing="t1"/>
      </pair>
      <pair id="p1">
        <zero timing="t1"/>
        <one timing="t1"/>
      </pair>
    </pairs>
    <intro/>
    <repeat>
      <coded_number direction="msb" zero="p0" one="p1" bits="1">
	<constant value="1"/>
      </coded_number>
      <coded_number direction="msb" zero="p0" one="p1" bits="1">
	<complement bits="1">
	  <mask mask="64" shift="-6">
	    <parameterref parameter="subdevice"/>
	  </mask>
	</complement>
      </coded_number>
      <coded_number direction="msb" zero="p0" one="p1" bits="1">
	<parameterref parameter="toggle"/>
      </coded_number>
      <coded_number direction="msb" zero="p0" one="p1" bits="5">
	<parameterref parameter="deviceno"/>
      </coded_number>
      <zero timing="t4"/>
      <coded_number direction="msb" zero="p0" one="p1" bits="6">
	<parameterref parameter="subdevice"/>
      </coded_number>
      <coded_number direction="msb" zero="p0" one="p1" bits="6">
	<parameterref parameter="cmdno"/>
      </coded_number>
      <zeropadding timing="ttotal"/>
    </repeat>
  </body>
</irprotocol>
