  <commandset type="serial" protocol="rs232_9600_8n1" prefix="@" suffix="\r">
    <command cmdref="cmd_power_toggle" name="Power" transmit="PWR:0" response_lines="1"/>
    <command cmdref="cmd_power_off" transmit="PWR:1" response_lines="1"/>
    <command cmdref="cmd_power_on"  transmit="PWR:2" response_lines="1"/>
    <command cmdref="get_power"  transmit="PWR:?" response_lines="1"/><!-- doc sez PWR? -->
    <command cmdref="cmd_mute_toggle" transmit="AMT:0" response_lines="1"/><!-- broken, returns status -->
    <command cmdref="cmd_mute_on" transmit="AMT:1" response_lines="1"/><!-- inverted semantic -->
    <command cmdref="cmd_mute_off" transmit="AMT:2" response_lines="1"/><!-- inverted semantic -->
    <command cmdref="cmd_stop" transmit="PMD:1" response_lines="1"/>
    <command cmdref="cmd_pause_on" transmit="PMD:2" response_lines="1"/>
    <command cmdref="cmd_play" transmit="PMD:3" response_lines="1"/>
    <command cmdref="get_mode" transmit="PMD:?" response_lines="1"/><!-- doc sez PMD? -->
    <command cmdref="cmd_next" transmit="GOT:0" response_lines="1"/>
    <command cmdref="cmd_previous" transmit="GOT:1" response_lines="1"/>
    <command cmdref="cmd_menu" transmit="MNU:0" response_lines="1"/>
    <command cmdref="cmd_up" transmit="CUR:1" response_lines="1"/>
    <command cmdref="cmd_down" transmit="CUR:2" response_lines="1"/>

    <command cmdref="cmd_osd" transmit="MOD:0" response_lines="1"/>
    <command cmdref="cmd_osd_on" transmit="MOD:1" response_lines="1"/>
    <command cmdref="cmd_osd_off" transmit="MOD:2" response_lines="1"/>
    <command cmdref="get_osd" transmit="MOD:?" response_lines="1"/>

    <command cmdref="cmd_ok" transmit="ENT:0" response_lines="1"/>

    <command cmdref="cmd_shuffle" transmit="RDM:0" response_lines="1"/>
    <command cmdref="cmd_shuffle_off" transmit="RDM:1" response_lines="1"/>
    <command cmdref="cmd_shuffle_song" transmit="RDM:2" response_lines="1"/>
    <command cmdref="cmd_shuffle_album" transmit="RDM:4" response_lines="1"/>
    <command cmdref="get_shuffle" transmit="RDM:?" response_lines="1"/>
    <command cmdref="cmd_repeat" transmit="REP:0" response_lines="1"/>
    <command cmdref="cmd_repeat_off" transmit="REP:1" response_lines="1"/>
    <command cmdref="cmd_repeat_track" transmit="REP:2" response_lines="1"/>
    <command cmdref="cmd_repeat_all" transmit="REP:4" response_lines="1"/>
    <command cmdref="get_repeat" transmit="REP:?" response_lines="1"/>

    <command cmdref="cmd_playlist_up" transmit="PLL:1" response_lines="1"/>
    <command cmdref="cmd_playlist_down" transmit="PLL:2" response_lines="1"/>
    <command cmdref="cmd_album_up" transmit="ABL:1" response_lines="1"/>
    <command cmdref="cmd_album_down" transmit="ABL:2" response_lines="1"/>

    <command cmdref="cmd_sort" transmit="SOS:0" response_lines="1"/>
    <command cmdref="cmd_sort_playlist" transmit="SOS:1" response_lines="1"/>
    <command cmdref="cmd_sort_artist" transmit="SOS:2" response_lines="1"/>
    <command cmdref="cmd_sort_album" transmit="SOS:3" response_lines="1"/>
    <command cmdref="cmd_sort_song" transmit="SOS:5" response_lines="1"/>
    <command cmdref="cmd_sort_audiobook" transmit="SOS:7" response_lines="1"/>
    <command cmdref="cmd_sort_podcast" transmit="SOS:8" response_lines="1"/>
    <command cmdref="get_sort" transmit="SOS:?" response_lines="1"/>
    <command cmdref="get_track" transmit="IND:?" response_lines="1"/>
    <command cmdref="get_database" transmit="NDB:?" response_lines="1"/>
    <command cmdref="get_title" transmit="SON:?" response_lines="1"/>
    <command cmdref="get_artist" transmit="ARN:?" response_lines="1"/>
    <command cmdref="get_album" transmit="ALN:?" response_lines="1"/>

    <command cmdref="get_contents_data1"  transmit="CD1:" response_lines="4"/>
    <command cmdref="get_contents_data2"  transmit="CD2:" response_lines="4"/>
    <command cmdref="get_contents_data3"  transmit="CD3:" response_lines="4"/>
    <command cmdref="get_contents_data4"  transmit="CD4:" response_lines="4"/>
    <command cmdref="get_contents_data_all"  transmit="CDR?" response_lines="4"/>
  </commandset>

