<device id="marantz_is201" name="Marantz IS201 iPod dock" vendor="Marantz" model="is201" type="misc_audio">
  <jp1data>
    <setupcode value="2020"/>
  </jp1data>
  <commandset type="ir" protocol="rc5" toggle="yes" deviceno="14" remotename="marantz_is201" original_remote="RC201IS">
    <command cmdref="power_toggle" name="power"	cmdno="12"/>
    <command cmdref="mute_toggle" name="power"	cmdno="13"/>
  </commandset>

  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="12" remotename="marantz_is201">
    <command cmdno="01" cmdref="power_on" remark="Power On iPod"/>
    <command cmdno="02" cmdref="power_off" remark="Power Off iPod (Charging)"/>
  </commandset>
  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="13" remotename="marantz_is201">
    <command cmdno="00" cmdref="mute_on" remark="Mute On"/>
    <command cmdno="01" cmdref="mute_off" remark="Mute Off"/>
  </commandset>
  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="28" remotename="marantz_is201">
    <command cmdno="01" cmdref="repeat" remark="Select Repeat mode (1Track > All > Repeat Off ) togle"/>
    <command cmdno="02" cmdref="repeat_track" remark="Repeat Track"/>
    <command cmdno="03" cmdref="repeat_all" remark="Repeat All"/>
    <command cmdno="04" cmdref="repeat_off" remark="Repeat Off"/>
  </commandset>
  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="29" remotename="marantz_is201">
    <command cmdno="01" cmdref="shuffle" remark="Select Shuffle mode (Song >Album > Shuffle Off) toggle"/>
    <command cmdno="02" cmdref="shuffle_songs" remark="Shuffle by Songs"/>
    <command cmdno="03" cmdref="shuffle_albums" remark="Shuffle by Albums"/>
    <command cmdno="04" cmdref="shuffle_off" remark="Shuffle Off"/>
  </commandset>
  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="13" remotename="marantz_is201">
    <command cmdno="01" cmdref="fast_forward" remark="Fast Forward"/>
  </commandset>
  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="31" remotename="marantz_is201">
    <command cmdno="01" cmdref="rewind"/>
  </commandset>
  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="32" remotename="marantz_is201">
    <command cmdno="01" cmdref="next" remark="Next Track"/>
    <command cmdno="02" cmdref="next_album" remark="Select Next Album"/>
    <command cmdno="03" cmdref="next_playlist" remark="Select Next Playlist"/>
  </commandset>
  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="33" remotename="marantz_is201">
    <command cmdno="01" cmdref="previous" remark="Previous Track"/>
    <command cmdno="02" cmdref="previous_album" remark="Select Previous Album"/>
    <command cmdno="03" cmdref="previous_playlist" remark="Select Previous Playlist"/>
  </commandset>
  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="36" remotename="marantz_is201">
    <command cmdno="01" cmdref="browse_remote" remark="Operation mode (Display OSD > Display LCD on iPod) toggle"/>
    <command cmdno="02" cmdref="browse_mode" remark="OSD Display mode (2way control)"/>
    <command cmdno="03" cmdref="remote_mode" remark="iPod LCD Display mode (Standard mode)"/>
  </commandset>
  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="41" remotename="marantz_is201">
    <command cmdno="01" cmdref="sort" remark="Sort mode (Artist/Album/Song/Genre/Composer/Play list/Podcasting/Audiobook) toggle"/>
    <command cmdno="02" cmdref="sort_artist" remark="Sort by Artist"/>
    <command cmdno="03" cmdref="sort_album" remark="Sort by Album"/>
    <command cmdno="04" cmdref="sort_song" remark="Sort by Song"/>
    <command cmdno="05" cmdref="sort_genres" remark="Sort by Genres"/>
    <command cmdno="06" cmdref="sort_composer" remark="Sort by Composer"/>
    <command cmdno="07" cmdref="sort_playlist" remark="Sort by Play-List"/>
    <command cmdno="08" cmdref="sort_podcast" remark="Sort by Podcasting"/>
    <command cmdno="09" cmdref="sort_audiobook" remark="Sort by Audio Books"/>
  </commandset>
  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="48" remotename="marantz_is201">
    <command cmdno="01" cmdref="pause" remark="Pause"/>
  </commandset>
  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="53" remotename="marantz_is201">
    <command cmdno="01" cmdref="play_pause" remark="Play/Pause toggle"/>
    <command cmdno="02" cmdref="play" remark="Play"/>
  </commandset>
  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="54" remotename="marantz_is201">
    <command cmdno="01" cmdref="stop"/>
  </commandset>
  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="80" remotename="marantz_is201">
    <command cmdno="01" cmdref="up" remark="Select contents up"/>
  </commandset>
  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="81" remotename="marantz_is201">
    <command cmdno="01" cmdref="down" remark="Select contents down"/>
  </commandset>
  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="82" remotename="marantz_is201">
    <command cmdno="01" cmdref="menu" remark="Menu or Return "/>
  </commandset>
  <commandset type="ir" protocol="rc5x" deviceno="14" subdevice="87" remotename="marantz_is201">
    <command cmdno="01" cmdref="ok" remark="Enter "/>
  </commandset>

